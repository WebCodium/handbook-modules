doctype html
html(lang="en", ng-app="demo", ng-cloak)
    head
        meta(charset="UTF-8")
        title Handbook Modules
        link(rel="stylesheet", href="/bower_components/bootstrap/dist/css/bootstrap.min.css")
        link(rel="stylesheet", href="/bower_components/angular-xeditable/dist/css/xeditable.css")
        link(rel="stylesheet", href="/public/style/main.css")
        script(src="/bower_components/jquery/dist/jquery.min.js")
        script(src="/bower_components/bootstrap/js/scrollspy.js")
        script(src="/bower_components/bootstrap/js/affix.js")

        script(src="/bower_components/angular/angular.js")
        script(src="/public/scripts/main.js")

        script(src="/bower_components/angular-xeditable/dist/js/xeditable.min.js")

        script(src="/bower_components/angular-ui-event/dist/event.min.js")
        script(src="/bower_components/angular-ui-map/ui-map.min.js")

        script(src="/dist/app.js")
        script(src="/dist/templates.js")
        body(data-spy="scroll" data-target="#navbar-example")
        div(class="container")
            include partials/navbar
            div.row
                div.col-md-2
                    include partials/sidebar
                div.col-md-10.frame-modules
                    div#panel-default
                        include partials/modules/panelDefault
                    div#address
                        include partials/modules/address
    script(src="https://maps.googleapis.com/maps/api/js?libraries=places&v=3.exp&sensor=false")
